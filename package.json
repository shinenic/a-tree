  "scripts": {
    "build:manifest": "node ./script/manifest.js",
    "build:dev": "yarn build:manifest && webpack --config=webpack/webpack.common.js --mode=development",
    "build:prod": "yarn build:manifest && webpack --config=webpack/webpack.common.js --mode=production",
    "dev": "LOCAL_MODE=false yarn build:dev --watch",
    "analyzer": "LOCAL_MODE=false ANALYZER=true yarn build:dev",
    "analyzer:prod": "LOCAL_MODE=false ANALYZER=true yarn build:prod",
    "dev:local": "LOCAL_MODE=true PORT=5566 API_PORT=5567 webpack-dev-server --config=webpack/webpack.local.js",
    "mockoon:start": "mockoon-cli start --data ./mockAPI/mockoon.json -i 0",
    "mockoon:stop": "mockoon-cli stop"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
